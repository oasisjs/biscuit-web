{
  "name": "@biscuit/web",
  "description": "Presentation page of the biscuit modules.",
  "author": "biscuitland",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "update": "pnpm up --interactive",
    "update:force": "pnpm up --latest",
    "postinstall": "nuxt prepare && husky install .husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@commitlint/format": "^17.4.0",
    "@nuxt/image-edge": "1.0.0-27840416.dc1ed65",
    "@nuxtjs/fontaine": "^0.2.4",
    "@nuxtjs/tailwindcss": "^6.2.0",
    "@nuxtjs/web-vitals": "^0.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "nuxt": "3.0.0",
    "nuxt-delay-hydration": "^1.0.5",
    "nuxt-icon": "^0.1.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "formatter": "@commitlint/format",
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack",
        "vue"
      ]
    },
    "allowedDeprecatedVersions": {
      "sourcemap-codec": "*",
      "rollup-plugin-terser": "*",
      "stable": "*"
    }
  },
  "packageManager": "pnpm@7.0.0"
}
